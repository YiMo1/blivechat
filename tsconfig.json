{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "temp",
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "newLine": "LF",
    "strict": true,
    "noEmit": true,
    "allowJs": false,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "useUnknownInCatchVariables": true,
    "allowImportingTsExtensions": true,
    "types": ["./global", "vite/client", "element-plus/global"],
    "jsxImportSource": "vue",
    "jsx": "preserve",
    "isolatedModules": true,
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src", "./auto-imports.d.ts"],
  "exclude": ["node_modules", "dist"]
}
